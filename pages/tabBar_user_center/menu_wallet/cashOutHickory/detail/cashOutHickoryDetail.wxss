page {
  background-color: #f4f4f4;
}

.head {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 32rpx 32rpx 0rpx 32rpx;
  padding: 64rpx;
  width: calc(100vw - 32rpx * 2 - 64rpx * 2);
  background: #ffffff;
  border-radius: 24rpx;
}

.head>image {
  width: 72rpx;
  height: 72rpx;
  margin-bottom: 32rpx;
}

.bottom {
  display: flex;
  flex-direction: column;
  margin: 0rpx 32rpx;
  padding: 64rpx 72rpx;
  width: calc(100vw - 32rpx * 2 - 72rpx * 2);
  background: #ffffff;
  border-radius: 24rpx;
  border-top: 1rpx #eee soild;
}

.textRow {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 12rpx;
}

.textRow>text:nth-of-type(1) {
  margin-right: 64rpx;
}