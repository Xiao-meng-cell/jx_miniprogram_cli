@import '/pages/tabBar_user_center/business_card_manage/business_card_manage_list/business_card_manage_list.wxss';

.order_contain {
  display: flex;
  flex-flow: column;
  flex-direction: column;
  height: 100%;
}

Page {
  width: 100%;
  height: 100%;
  background-color: white;
}

.card_userhaedImg {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: relative;
  overflow: hidden;
  padding: 30rpx 30rpx 15rpx 30rpx;
}

.card_list_item {
  background-color: white;
  width: 100%;
  height: auto;
  position: relative;
  border-radius: 10rpx;
  overflow: hidden;
  margin: auto;
  box-shadow: 0px 0rpx 10rpx 2rpx #00000029;
  margin-top: 20rpx;
}
