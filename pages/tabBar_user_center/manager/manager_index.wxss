/* miniprogram/pages/tabBar_user_center/manager/manager_index.wxss */

page {
  background-color: #f6f6f6;

}

.header_gradient {
  width: 100%;
  position: relative;
  /* background: -webkit-linear-gradient(#494fad, #d03eab, white);
  background: -o-linear-gradient(#494fad, #d03eab, white);
  background: -moz-linear-gradient(#494fad, #d03eab, white);
  background: linear-gradient(#494fad, #d03eab, white); */
}

.header_gradient_masking {
  position: absolute;
  top: 220rpx;
  width: 100%;
  height: 160rpx;
  background: linear-gradient(to bottom, rgba(255, 255, 255, 0), rgba(246, 247, 248, 1));
}

.header_bg {
  width: 100%;
  height: 372rpx;
  left: 0;
  right: 0;
}

.order_header {
  display: flex;
  align-items: center;
  flex-direction: row;
  justify-content: center;
  border-radius: 24rpx;
  background: white;
  position: absolute;
  bottom: -250rpx;
  right: 32rpx;
  left: 32rpx;
}

.to_detail {
  right: 15px;
  top: 15px;
  position: absolute;
}

.to_detail>image {
  width: 64rpx;
  height: 64rpx;
}

.avatar_content {
  align-items: center;
  flex-direction: column;
  justify-content: center;
}

.port {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.order_menu_content {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 1;
  justify-content: center;
}

.order_menu_content>text {
  margin-top: 20rpx;
}

.order_menu_content2 {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 19rpx 10rpx 19rpx 16rpx;
  background: #F6F6F6;
  border-radius: 24rpx;
}

.order_menu_content2>view {
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
}

.order_menu_content2>view>text:nth-of-type(1) {
  font-size: 24rpx;
  color: #37424D;
  margin-bottom: 4rpx;
}

.order_menu_content2>view>text:nth-of-type(2) {
  font-size: 22rpx;
  color: #A0A0A0;
}

.order_menu_txt {
  font-size: 22rpx;
  display: block;
  margin-top: 16rpx;
}

.order_menu_icon {
  width: 48rpx;
  height: 48rpx;
}

.group_margin {
  padding: 20rpx 0rpx 0rpx 0rpx;
  font-size: 28rpx;
  color: black;
  width: 100%;
  box-sizing: border-box;
}

.tag_icon {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background: white;
  margin-right: 10rpx;
}

.opacity_bg {
  opacity: 0.2;
  height: 52rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  background: #ffb;
  border-bottom-left-radius: 26rpx;
  border-top-left-radius: 26rpx;
  border-bottom-right-radius: 26rpx;
  border-top-right-radius: 26rpx;
}

.align_center {
  align-items: center;
  display: flex;
  flex-direction: row;
}

.mid_contain {
  display: flex;
  flex-direction: row;
  align-items: center;
  /*垂直居中*/
  background: white;
}

.mid_align_center {
  display: flex;
  flex: 1;
  padding: 20rpx;
  align-items: center;
  flex-direction: column;
  justify-content: center;
}

/**
 * 约会菜单部分
 **/

.big_order_menu_contain {
  background: white;
  width: 100%;
  border-top-width: 2rpx;
  border-top-color: #efeff4;
  border-bottom-width: 2rpx;
  border-bottom-color: #efeff4;
  border-top-style: solid;
  border-bottom-style: solid;
}

.big_order_menu_title {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding-left: 30rpx;
  padding-right: 40rpx;
  padding-bottom: 20rpx;
  padding-top: 20rpx;
}

.big_order_menu {
  display: flex;
  flex-direction: column;
  width: calc(100% - 24rpx * 2);
  padding: 24rpx 24rpx 32rpx 24rpx;
  color: #37424D;
}

.big_order_menu_name_status {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.merchantNameStatus {
  max-width: calc(100% - 14rpx - 32rpx - 20rpx * 6.5);
}

.merchantNameTag {
  max-width: calc(100% - 48rpx - 8rpx);
}

.merchantNameTagStatus {
  max-width: calc(100% - 48rpx - 8rpx - 14rpx - 32rpx - 20rpx * 6.5);
}

.big_order_menu_name_status>text:nth-child(1) {
  font-size: 32rpx;
  font-weight: bold;
}

.big_order_menu_name_status_status {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 14rpx;
}

.big_order_menu_name_status_status>image {
  width: 32rpx;
  height: 32rpx;
}

.big_order_menu_name_status_status>text {
  position: relative;
  left: -8rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  padding: 7rpx 8rpx 7rpx 16rpx;
  border-top-right-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}

.big_order_menu_status_name_color {
  color: #2F95FB;
  background-color: rgba(47, 149, 251, 0.05);
}

.big_order_menu_status_name_color2 {
  color: #FF6F00;
  background-color: rgba(255, 111, 0, 0.05);
}

.big_order_menu_name_status>image:nth-last-of-type(1) {
  width: 48rpx;
  height: 24rpx;
  margin-left: 8rpx;
}


.big_order_menu_tag_time {
  display: flex;
  flex-direction: row;
  height: 34rpx;
  margin-top: 16rpx;
  margin-bottom: 24rpx;
}

.big_order_menu_tag_time>text:nth-of-type(1) {
  font-size: 24rpx;
  padding-right: 24rpx;
  font-weight: bold;
  border-right: #EEEEEE 2rpx soild;
}

.big_order_menu_tag_time>text:nth-of-type(2) {
  font-size: 24rpx;
  font-weight: bold;
  padding-left: 24rpx;
}

.big_order_menu_racking {
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
  color: #a0a0a0;
}

.big_order_menu_racking_fgx {
  color: #EEEEEE;
  margin-left: 16rpx;
  margin-right: 16rpx;
}

.big_order_menu_fgx {
  width: 100%;
  height: 2rpx;
  background: #EEEEEE;
  margin-top: 24rpx;
  margin-bottom: 32rpx;
}

.top_padding {
  padding: 0px;
}

.endTime {
  display: flex;
  flex-direction: row;
  position: absolute;
  justify-content: center;
  text-align: center;
  width: 100%;
  bottom: 5vh;
}

.checkOut {
  display: flex;
  flex-direction: row;
  width: 100%;
  justify-content: center;
  align-items: center;
  margin-bottom: 26rpx;
  margin-top: 40rpx;
}

.checkOut>text {
  color: #D8D8D8;
  font-size: 20rpx;
  text-decoration: underline;
}

.viewCheckOut {
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(17, 12, 12, 0.6);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
}

.viewCheckOutCode {
  display: flex;
  flex-direction: column;
  position: fixed;
  background-color: #fff;
  padding: 24rpx;
  width: 80vw;
  border-radius: 16rpx;
  justify-content: center;
  align-items: center;
}

.viewCheckOutCode>view:nth-of-type(1) {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.viewCheckOutCode input {
  width: calc(100% - 24rpx * 3 - 260rpx);
  height: 80rpx;
  border: 2rpx #000 solid;
  border-radius: 16rpx;
  font-size: 32rpx;
  padding-left: 24rpx;
  padding-right: 24rpx;
}

.auxFunc {
  display: flex;
  flex-direction: column;
  width: calc(100vw - 32rpx * 4);
  margin-top: 24rpx;
  background-color: #fff;
  border-radius: 24rpx;
  margin-left: 32rpx;
  margin-right: 32rpx;
  padding: 32rpx;
}

.auxFunc>text {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 32rpx;
}

.auxFuncItem {
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 25%;
  height: 100%;
}

.newTag {
  position: absolute;
  right: 20%;
  top: -10rpx;
  padding: 4rpx 8rpx;
  background: #FF2D55;
  color: #fff;
  font-size: 16rpx;
  border-radius: 14rpx;
}

.tipBG {
  display: flex;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
  justify-content: center;
  align-items: center;
}

.tipContent {
  display: flex;
  flex-direction: column;
  margin-left: 96rpx;
  margin-right: 96rpx;
  background-color: #fff;
  border-radius: 18rpx;
  align-items: center;
}

.tipContent>view:nth-child(1) {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  border-top-left-radius: 24rpx;
  border-top-right-radius: 24rpx;
}

.tipContent>view:nth-child(1)>text:first-child {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 40rpx;
  margin-bottom: 42rpx;
}

.tipContent_text {
  font-size: 28rpx;
  color: #37424D;
  margin: 0rpx 42rpx 0rpx 42rpx;
}

.tipContent_text2 {
  font-size: 28rpx;
  margin: 24rpx 42rpx 0rpx 42rpx;
  color: #2F95FB;
}

.tips_fgx {
  width: 90%;
  height: 2rpx;
  background-color: #eee;
  margin-top: 38rpx;
}

.submit {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 24rpx;
  margin-bottom: 24rpx;
}

.order_content {
  margin-top: 250rpx;
}

.imgServiceTips {
  width: calc(100vw - 32rpx * 2);
  margin-top: 24rpx;
}

.olPayentry {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 80rpx;
  background-image: url(https://weclubbing.oss-cn-shenzhen.aliyuncs.com/static/miniProgramBusiness/assets/enterRenewsUpgrade/entryOnlinePay.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.olPayentry>view {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 40rpx;
  margin-left: 160rpx;
  margin-right: 64rpx;
}

.olPayentry>view>text:nth-of-type(1) {
  font-size: 22rpx;
  color: #fff;
}

/* 剩余支付时间 start */
.time {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 22rpx;
}

.time>view {
  margin-right: 4rpx;
}

.time>view text {
  margin-left: 4rpx;
  color: #fff;
}

.time>view>text:nth-of-type(2) {
  margin-right: 4rpx;
}

.timeItem {
  font-size: 20rpx;
  color: #ffffff;
  padding: 0px 6rpx;
  background: #37424D;
  border-radius: 8rpx;
}

/* 剩余支付时间 end */

.other_func {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  /* margin-bottom: 32rpx; */
}

.other_func>text:nth-of-type(1) {
  font-size: 28rpx;
  font-weight: bold;
}

.other_func>text:nth-of-type(2) {
  color: #5ABDFF;
  font-size: 22rpx;
}


/* 合同协议 start */
.agreement {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.3);
}

.agreement>view {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 32rpx;
  width: calc(80vw - 32rpx * 2);
  background: #fff;
}

.agreementBottomBar {
  margin-top: 64rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.agreementBottomBar>view {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  padding-left: 32rpx;
  padding-right: 32rpx;
  border-radius: 44rpx;
  height: 88rpx;
  font-size: 28rpx;
  color: #fff;
}

.agreementBottomBar>view:not(:first-child) {
  margin-left: 32rpx;
}

.agreementTitle {
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  font-weight: bold;
}

.agreementSubTitle {
  font-size: 28rpx;
  margin-top: 32rpx;
}

.agreementCheck {
  display: flex;
  flex-direction: column;
  width: 100%;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}

.agreementDownload {
  position: absolute;
  right: 32rpx;
  top: 40rpx;
  width: 32rpx;
  height: 32rpx;
}

/* 合同协议 end */