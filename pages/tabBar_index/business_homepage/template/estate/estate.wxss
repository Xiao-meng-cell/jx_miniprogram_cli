/* 吸顶bar start */

.navbar-wrap {
  width: 100%;
}

.navbar-wrap .column {
  width: 100%;
  background: #fff;
  border-bottom: solid 2rpx #eee;
  top: 0;
  left: 0;
  z-index: 9999;
}

.navbar-wrap .column.fixed {
  position: fixed;
}

.navbar-wrap .col {
  height: 150rpx;
  display: flex;
  flex-flow: column;
  justify-content: space-between;
}

.navbar-wrap .row {
  display: flex;
  flex-flow: row;
  justify-content: space-around;
}

.navbar-wrap .title {
  width: 100%;
  height: 88rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

/* 吸顶bar end */

/* 货源banner start  */

.sourceBanner {
  height: 160rpx;
  margin-top: 20rpx;
  display: flex;
  flex-flow: column;
  justify-content: center;
  padding: 0px 32rpx;
  background-image: url(https://izsyue.oss-cn-shenzhen.aliyuncs.com/assets/applets/estatePackage/huoyuan-bg%403x.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.sourceBanner .btn {
  width: 180rpx;
  margin-top: calc(160rpx / 2);
  text-align: center;
  border-radius: 28rpx;
  padding: 5rpx 20rpx;
  background: #2F95FB;
}

/* 货源banner end */


/* 房产项目 star */
.estateProjectList {
  margin-top: 40rpx;
  width: 100%;
  height: 116rpx;
}

.estateProjectList .scroll {
  white-space: nowrap;
  display: flex;
}

.estateProjectList .scroll .rowItem {
  display: inline-block;
  margin: 0 10rpx;
  border-radius: 24rpx;
}

.estateProjectList .scroll .rowItem .image {
  width: 128rpx;
  height: 98rpx;
  border-radius: 16rpx;
}

.estateProjectList .scroll .rowItem .active {
  width: 160rpx;
  height: 116rpx;
  border-radius: 16rpx;
}

.estateProjectList .rowItem .title {
  width: 128rpx;
  padding: 10rpx;
  margin-top: -40rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.estateProjectInfo {
  width: 100%;
  margin-top: 20rpx;
  display: flex;
  flex-flow: column;
  justify-content: center;
  padding: 32rpx 24rpx;
  background: #fff;
  border-radius: 12px;
}

.estateProjectInfo .row {
  width: 100%;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  align-items: flex-start;
}

.estateProjectInfo .tag {
  text-align: center;
  border-radius: 25rpx;
  background: #2F95FB;
  padding: 0 10rpx;
  margin-right: 15rpx;
}


.estateProjectInfo .title {
  font-weight: bold;
  margin-bottom: 20rpx;
}

.estateProjectInfo .name {
  margin-right: 20rpx;
}

.estateProjectInfo .address {
  width: 100%;
}

.estateProjectInfo .icon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}

.estateProjectInfo .tagList {
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  flex-flow: row wrap;
}

.estateProjectInfo .tagList .tagItem {
  margin: 10rpx 0;
  width: 50%;
}

/* 房产项目 end */
/* 项目周边 start */
.projectBtn {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 88rpx;
  border-radius: 88rpx;
  background: #2F95FB;
  margin-top: 50rpx;
}


/* goodsBar start  */
.goodsBar {
  margin-top: 50rpx;
