}

/* 楼市资料 end */

/* 楼市资讯 start */
.newsBar {
  margin-top: 20rpx;
  width: 100%;
  height: 120rpx;
  background: #fff;
  border-radius: 12px;
  align-items: center;
  padding: 20rpx 20rpx 20rpx 30rpx;
  display: flex;
  flex-flow: row;
}

.newsBar .left {
  width: 72rpx;
  height: 72rpx;
  margin-right: 30rpx;
}

.newsBar .row {
  display: flex;
  flex-flow: row;
}

.newsBar .col {
  display: flex;
  flex-flow: column;
  height: 100%;
  justify-content: space-around;
}

.newsBar .icon {
  width: 25rpx;
