#main {
  background-color: #f6f6f6;
}

.notPass {
  position: absolute;
  top: 12rpx;
  right: calc(32rpx + 50rpx * 2);
  z-index: 1000;
  width: 112rpx;
  height: 112rpx;
}

.home_bg {
  position: relative;
  background-color: #eee;
  width: 750rpx;
  height: 750rpx;
}

.home_bg > swiper image {
  width: 100%;
  height: 100%;
  background-color: #eee;
  min-width: 376rpx;
  min-height: 370rpx;
  position: absolute;
}

.home_bg > swiper {
  width: 100%;
  height: 100%;
  background-color: #eee;
  min-width: 376rpx;
  min-height: 370rpx;
  position: absolute;
}

.home_bg > text:nth-child(3) {
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  background-color: rgba(158, 158, 158, 0.78);
  border-radius: 10rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  color: white;
  font-size: 24rpx;
}

.red_mask {
  position: relative;
  display: flex;
  justify-content: space-between;
  width: 100%;
  background-color: #fff;
}

.red_mask_left {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  margin-top: 24rpx;
}

.red_mask_left > view:nth-child(1) {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  width: 100%;
  font-weight: bold;
}

.red_mask_left > view:nth-child(1) > text:nth-child(1) {
  font-size: 36rpx;
  line-height: 36rpx;
  margin-right: 0rpx;
}

.red_mask_left > view:nth-child(1) > text:nth-child(2) {
  font-size: 48rpx;
  line-height: 48rpx;
}

.red_mask_left > view:nth-child(2) {
  height: 36rpx;
  width: 100%;
  overflow: hidden;
  display: flex;
  color: #a0a0a0;
  font-size: 24rpx;
  margin-top: 8rpx;
}

/* 参考价 */
.reference-price {
  position: relative;
  color: #a0a0a0;
  margin-left: 16rpx;
  font-size: 22rpx;
}

.reference-price::after {
  content: "";
  flex: 1;
  height: 2rpx;
  background: #a0a0a0;
  width: 100%;
  position: absolute;
  content: "";
  top: 18rpx;
  left: 0px;
}

.estateDepositTag {
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 48rpx;
  margin-left: 16rpx;
}

.estateDepositTag > text:nth-child(1) {
  font-size: 20rpx;
  line-height: 20rpx;
  color: #fff;
  background: linear-gradient(142deg, #fec673, #ffb74d);
  padding: 10rpx 10rpx 10rpx 10rpx;
  border-radius: 20rpx;
  z-index: 2;
}

.estateDepositTag > text:nth-child(2) {
  font-size: 20rpx;
  margin-left: -16rpx;
  padding-left: 20rpx;
  padding-right: 8rpx;
  color: #ffb74d;
  background: rgba(255, 111, 0, 0.05);
  border-radius: 14rpx;
}

.red_mask_right {
  display: flex;
  flex-direction: column;
  margin-top: 24rpx;
}

.countdown > text {
  font-size: 24rpx;
  line-height: 34rpx;
}

.countdown > view:nth-child(1) {
  font-size: 24rpx;
  text-align: right;
  margin-bottom: 8rpx;
  color: #37424d;
}

.countdown > view:nth-child(2) {
  height: 32rpx;
  font-size: 22rpx;
  text-align: right;
  margin-top: 8rpx;
  color: #37424d;
}

.countdown > view:nth-child(2) > text {
  color: white;
  background-color: rgb(50, 50, 50);
  padding-top: 6rpx;
  padding-bottom: 4rpx;
  padding-left: 6rpx;
  padding-right: 6rpx;
  border-radius: 10rpx;
}

.countdown > view:nth-child(2) > span {
  margin-left: 4rpx;
}

.countdown > view:nth-child(2) > span:not(:last-child) {
  margin-right: 4rpx;
}

