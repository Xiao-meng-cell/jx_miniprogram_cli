.drawer_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  
}
.drawer_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 1001;
  background: #fafafa;
  margin: -300rpx 0 0 0;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
  padding: 0;
  background-repeat: no-repeat;
  background-size:contain;
  opacity: 0;
  background-position: center center;
  background-color: rgba(255,255,255,0);
  background-image:url("data:image/png;base64,");
}
.drawer_title {
  padding: 30rpx;
  font: 40rpx "microsoft yahei";
  text-align: center;
}
.drawer_content {
  height: 500rpx;
  overflow-y: scroll; /*超出父盒子高度可滚动*/
  text-align: center;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
  display:flex;
}

