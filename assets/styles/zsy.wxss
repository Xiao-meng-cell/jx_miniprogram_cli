/**
 * 超出一行文字自动隐藏
 * 文字隐藏后添加省略号
 * 强制不换行
 * 容器需要设置overflow: hidden;
 **/
.alone-line{
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap; 
}

/**水平方向**/
.horizontal-htop {
  display: flex;
  flex-direction: row;
}
/**垂直方向**/
.vertical-left {
  display: flex;
  flex-direction: column;
}

/*水平垂直居中对齐*/
.horizontal-vcenter {
  display: flex;
  flex-direction: row;
  align-items: center; /*垂直居中*/
}

/*垂直水平居中对齐*/
.vertical-hcenter {
  display: flex;
  flex-direction: column;
  align-items: center; /*垂直居中*/
  justify-content: center; /*水平居中*/
}


