  height: 32rpx;
  border-radius: 8rpx;
  margin-right: 8rpx;
}

.goodsInfo>view:nth-child(1)>text {
  width: calc(100% - 32rpx - 8rpx * 3 - 56rpx * 2);
  margin-right: 16rpx;
}


.goodsInfo>view:nth-child(1)>image:nth-of-type(2) {
  width: calc(56rpx * 2);
  height: calc(20rpx * 2);
}

.goodsInfo_goods {
  display: flex;
  flex-direction: column;
  padding-top: 40rpx;
}

.goodsInfo_goods_info {
  display: flex;
  flex-direction: row;
}

.goodsInfo_goods_info>view:nth-of-type(1) {
  display: flex;
  flex-direction: column;
}

.goodsInfo_goods_info>view>view:nth-child(1) {
  display: flex;
  flex-direction: row;
}

.goodsInfo_goods_info>view>view:nth-child(1)>view:nth-child(3) {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}

.goodsInfo_goods_info>view>view:nth-child(2) {
  font-size: 24rpx;
  color: #a0a0a0;
  margin-top: 16rpx;
}

.goodsInfo_goods_info>view>view:nth-child(3) {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
