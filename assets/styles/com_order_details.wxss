/* miniprogram/pages/order/bepaid/bepaid.wxss */


.order_addr_nav {
  display: -webkit-box;
  /**定义为自适应布局*/
  word-break: break-all;
  text-overflow: ellipsis;
  /**超出文字用省略号代替*/
  overflow: hidden;
  /**多出部分隐藏*/
  -webkit-box-orient: vertical;
  /**竖直方向的超出和隐藏*/
  -webkit-line-clamp: 2;
  /**设定行数为2*/
  color: #323232;
  flex: 1;
}

.order_blogo {
  width: 40rpx;
  height: 40rpx;
  margin: 20rpx;
}

.mid_order_bdes {
  display: flex;
  flex-direction: column;
  padding: 20rpx 20rpx 20rpx 20rpx;
}

.item_order_blogo {
  width: 44rpx;
  height: 44rpx;
}

.item_order_bname {
  /**企业名称*/
  color: #323232;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.item_order_gname {
  /**商品名称*/
  display: -webkit-box;
  /**定义为自适应布局*/
  word-break: break-all;
  text-overflow: ellipsis;
  /**超出文字用省略号代替*/
  overflow: hidden;
  /**多出部分隐藏*/
  -webkit-box-orient: vertical;
  /**竖直方向的超出和隐藏*/
  -webkit-line-clamp: 2;
  /**设定行数为1*/
  font-size: 24rpx;
  color: #37424d;
  margin-right: 44rpx;
}

.item_order_desc {
  display: -webkit-box;
  /**定义为自适应布局*/
  word-break: break-all;
  text-overflow: ellipsis;
  /**超出文字用省略号代替*/
  overflow: hidden;
  /**多出部分隐藏*/
  -webkit-box-orient: vertical;
  /**竖直方向的超出和隐藏*/
  -webkit-line-clamp: 2;
  /**设定行数为1*/
  font-size: 24rpx;
  color: #a0a0a0;
  margin-bottom: 20rpx;
}

.item_order_pimg {
  /**商品图片*/
  width: 180rpx;
  height: 180rpx;
  margin-right: 24rpx;
  border-radius: 16rpx;
}

.item_order_status_tips {
  font-size: 24rpx;
  color: #fb5c56;
}

.item_order_destotal {
  font-size: 24rpx;
  color: #323232;
}

.item_order_timeouttips {
  font-size: 22rpx;
  color: #fb5c56;
}

.btn_opt_contain {
  font-size: 24rpx;
  text-align: center;
  align-content: center;
  line-height: 56rpx;
  height: 56rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  width: auto;
  padding: 0 20rpx;
}

.btn_blue_border {
  border: 2rpx solid #5b86fe;
  color: #5b86fe;
  background: #fff;
}

.btn_grey_border {
  border: 2rpx solid #a0a0a0;
  color: #a0a0a0;
  background: #fff;
}

.btn_blue_bg_border {
  background-color: #5b86fe;
  border: 2rpx solid #5b86fe;
  color: #fff;
}

.btn_red_bg_border {
  background-color: #fff8f8;
  border: 2rpx solid #fb5c56;
  color: #fb5c56;
  font-size: 24rpx;
  text-align: center;
  line-height: 56rpx;
  height: 56rpx;
  width: auto;
  padding: 0px 20rpx;
}

.item_order_bnickname {
  /**用户昵称*/
  line-height: 28rpx;
  font-size: 28rpx;
  color: #323232;
  margin: 0 20rpx;
}

.item_order_time_tips {
  font-size: 20rpx;
  color: #a0a0a0;
}

.item_order_blogistics {
  border-radius: 16rpx;
  width: 32rpx;
  height: 32rpx;
}




/* 猜你喜欢 */

.status_icon {
  width: 140rpx;
  height: 140rpx;
  margin-top: 60rpx;
}

.status_title {
  font-size: 32rpx;
  margin-top: 40rpx;
  color: #323232;
}

.status_tip {
  font-size: 24rpx;
  margin-top: 30rpx;
  color: #a0a0a0;
}

.look-btn {
  width: 95%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin-top: 60rpx;
  border-radius: 6rpx;
  box-sizing: border-box;
  background-color: #5b86fe;
}

.list_contain {
  overflow: hidden;
  margin-top: 20rpx;
}

.home_grid {
  position: relative;
  float: left;
  padding: 20rpx;
  width: 50%;
  box-sizing: border-box;
}

.home_icon {
  width: 30rpx;
  height: 500rpx;
  justify-content: center;
  display: flex;
}

.home_grid_text {
  color: #323232;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.image_contain {
  width: 100%;
  min-height: 300rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  overflow: hidden;
  position: relative;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  border-radius: 20rpx;
  border: 1rpx solid #5b86fe;
  box-shadow: 0 6rpx 16rpx #e4e4e4;
}

.image_contain_text_contain {
  position: absolute;
  padding: 4rpx 16rpx;
  color: white;
  right: 20rpx;
  top: 20rpx;
  background-color: black;
  opacity: 0.5;
  border-radius: 30rpx;
  font-size: 22rpx;
}

/* 留言文本 */

.input_contain {
  height: 100rpx;
  font-size: 30rpx;
  padding-top: 10rpx;
  margin: 0 20rpx 0 0;
  width: 98%;
}

page {
  background: #f6f6f6;
}

/* 背景、状态提示 start  */
.main>image:nth-child(1) {
  position: relative;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 220rpx;
}

.orderStatus {
  position: relative;
  top: -176rpx;
  left: 56rpx;
  display: flex;
  flex-direction: column;
  width: calc(100vw - 56rpx * 2);
  color: #fff;
}

.orderStatus>text:nth-child(1) {
  font-size: 32rpx;
  font-weight: bold;
  margin-bottom: 16rpx;
}

.orderStatus>text:nth-child(2) {
  font-size: 24rpx;
}

.orderStatus>text:nth-child(3) {
  font-size: 20rpx;
  margin-top: 10rpx;
}

/* 背景、状态提示 end  */

/* 内容信息 start  */
.content {
  position: relative;
  left: 32rpx;
  /* top: -135rpx; */
  top: -155rpx;
  display: flex;
  flex-direction: column;
  width: calc(100vw - 32rpx * 2);
}

.content>view {
  background: #fff;
  border-radius: 24rpx;
  padding: 32rpx 24rpx;
  width: calc(100% - 24rpx * 2);
  margin-bottom: 24rpx;
}

/* 地址信息 start */
.addrInfo>view>image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 32rpx;
  flex-shrink: 0;
}

.addrInfo>view>view:nth-of-type(1) {
  display: flex;
  flex-direction: column;
  color: #37424d;
}

.addrInfo_service {
  width: calc(100% - 40rpx - 32rpx - 48rpx - 24rpx * 2);
  margin-right: 40rpx;
}

.addrInfo>view>view:nth-of-type(1)>text:nth-child(1) {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 16rpx;
}

.addrInfo>view>view:nth-of-type(1)>text:nth-child(2) {
  font-size: 24rpx;
}

.addrInfo>view>text:nth-of-type(1) {
  font-size: 24rpx;
  color: #2f95fb;
}

.addrInfo>view>view:nth-child(3) {
  padding: 2rpx 16rpx 2rpx 16rpx;
  border: 2rpx solid #2f95fb;
  border-radius: 32rpx;
}

.addrInfo>view>view:nth-child(3)>text {
  color: #2f95fb;
  font-size: 20rpx;
  line-height: 30rpx;
}

/* 地址信息 end  */

/*配送信息*/
.delivery_time {
  font-size: 28rpx;
}

.delivery_time image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 10rpx 0 0;
}

.delivery_time .time_box {
  max-width: 400rpx;
  font-size: 24rpx;
  text-align: right;
}

/* 订单用户 start  */
.orderUser {
  display: flex;
  flex-direction: column;
  color: #37424d;
}

.orderUser>text {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 16rpx;
}

.orderUser>view {
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
}

.orderUser>view>text:nth-child(1) {
  margin-right: 32rpx;
}

.orderUser>view>view:nth-child(4) {
  border: 2rpx solid #2f95fb;
  border-radius: 30rpx;
  font-size: 20rpx;
  line-height: 30rpx;
  color: #2f95fb;
  padding-left: 16rpx;
  padding-right: 16rpx;
}

/* 订单用户 end  */

/* 券码 start  */
.CDKEY {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  color: #37424d;
}

.CDKEY>view {
  display: flex;
  flex-direction: column;
}

.CDKEY>view>text:nth-child(1) {
  font-size: 28rpx;
  font-weight: bold;
  margin-bottom: 16rpx;
}

.CDKEY>view>text:nth-child(2) {
  font-size: 24rpx;
  color: #A0A0A0;
}

.CDKEY>image {
  width: 40rpx;
  height: 40rpx;
}

/* 券码 end  */

/* 配送记录 start  */

.logisticsInfo>image:nth-child(1) {
  width: 40rpx;
  height: 40rpx;
  margin-right: 32rpx;
}

.logisticsInfo>view:nth-child(2) {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: calc(100% - 40rpx - 32rpx * 2 - 20rpx);
  font-size: 24rpx;
  color: #37424d;
  margin-right: 32rpx;
}

.logisticsInfo>view:nth-child(2)>text:nth-child(2) {
  margin-top: 4rpx;
  color: #a0a0a0;
}

.logisticsInfo>image:nth-child(3) {
  width: 20rpx;
  height: 20rpx;
}

/* 配送记录 end  */

/* 商品信息 start  */

.goodsInfo {
  display: flex;
  flex-direction: column;
  color: #37424d;
  font-size: 24rpx;
}

.goodsInfo>view:nth-child(1) {}

.goodsInfo>view:nth-child(1)>image:nth-of-type(1) {
