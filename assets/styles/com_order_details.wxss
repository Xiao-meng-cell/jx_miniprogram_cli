}

/* 备注 end  */

/* 订单信息 start */
.orderInfo>text {
  color: #37424d;
  font-size: 28rpx;
}

.orderInfo>view {
  display: flex;
  flex-direction: column;
  margin-top: 32rpx;
  padding-top: 24rpx;
